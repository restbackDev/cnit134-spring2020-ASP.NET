@{
    ViewData["Title"] = "Welcome";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewData["Title"]</title>
</head>
<body>
    <h2>Welcome</h2>

    <ul>
        @foreach (var i in (List<Course>)ViewData["Message"])
        {
            <li>@i.Title - @i.StartDate.ToShortDateString() - @i.Type</li>
        }
    </ul>
</body> 
</html>
